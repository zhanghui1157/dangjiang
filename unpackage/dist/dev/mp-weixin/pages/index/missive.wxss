
.search-content{height: 100rpx;width: 100%;background: #f3f3f3;padding: 20rpx;}
.search-text{width: 96%;height: 60rpx;border-radius: 40rpx;background: #fff;margin: auto;padding:0 20rpx;font-size: 14px;}
.nav-list{width: 100%;display: -webkit-box;display: -webkit-flex;display: flex;height: 70rpx;-webkit-box-pack: justify;-webkit-justify-content: space-between;justify-content: space-between;border-bottom: 1px solid #e8e8e8;}
.nav-item{-webkit-box-flex: 1;-webkit-flex: 1;flex: 1;}
.nav-text{width: 100%;height: 60rpx;text-align: center;line-height: 70rpx;}
.nav-line{width:40%;margin: auto;border-top: 1px solid #e93a30;}
.item-list{width: 100%;border-bottom: 1px solid #e8e8e8;padding: 10rpx 0;line-height: 50rpx;}
.item-list:last-child{border-bottom:0rpx;}
.list-title{height: 50;font-weight: 500;color: #000;}
.list-time{width: 100%;text-align: right;font-size: 12px;color: #B3B3B3;}

