<view class="content"><view class="page-section swiper"><view class="page-section-spacing"><swiper class="swiper" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}"><block wx:for="{{turn}}" wx:for-item="item" wx:for-index="__i0__"><swiper-item><view class="swiper-item uni-bg-red"><image src="{{item}}" class="_img"></image></view></swiper-item></block></swiper></view></view><block wx:if="{{exam_list}}"><view><block wx:for="{{exam_list}}" wx:for-item="item" wx:for-index="__i1__"><view class="test-list"><view class="test-title">{{item.exam_title}}</view><view class="test-info"><view class="test-point">{{"积分："+item.exam_integral}}</view><view class="test-time">{{"考试时长："+item.exam_time+"分钟"}}</view><view class="test-end">{{"截止日期："+item.exam_date}}</view></view><view data-event-opts="{{[['tap',[['startTest',['$0'],[[['exam_list','',__i1__,'exam_id']]]]]]]}}" class="test-btn" bindtap="__e">开始考试</view><view class="clear"></view></view></block></view></block><block wx:else><view class="no-data"><view class="no-data-icon icon iconfont">;</view><view class="no-data-text">暂无数据</view></view></block><view class="footer"></view></view>