<view class="content"><view class="report-head"><view class="report-left"><view class="report-title">{{detail.worklog_title}}</view><view class="report-checker">{{"审核人:"+detail.audit_man}}</view></view><view class="report-right"><view class="report-time">{{detail.worklog_date}}</view><view class="report-type">{{detail.worklog_type}}</view></view></view><view class="middle-line"></view><block wx:if="{{detail.file}}"><view class="form-item"><view class="file-title">附件</view><block wx:for="{{detail.file}}" wx:for-item="item" wx:for-index="__i0__"><view class="file-list"><view class="file-name">{{item.upload_source}}</view><view class="file-size">{{item.upload_size}}</view><view data-event-opts="{{[['tap',[['download',['$0'],[[['detail.file','',__i0__,'upload_path']]]]]]]}}" class="file-del icon iconfont" bindtap="__e"></view></view></block></view></block><view class="middle-line"></view><view class="report-content"><rich-text nodes="{{detail.worklog_summary}}"></rich-text></view></view>