
page{background:#f3f3f3;}
.test-list{width: 94%;border-radius: 10rpx;padding: 15rpx 3%;margin:auto;background: #fff;margin-top: 20rpx;}
.test-title{width: 100%;font-size: 14px;font-weight: bold;line-height: 50rpx;}
.test-range,.test-point,.test-time,.test-end{font-size: 14px;line-height: 40rpx;}
.test-info{width: 60%;float: left;}
.question-list{width: 94%;margin:20rpx auto;background: #fff;border-radius: 10rpx;padding: 15rpx 25rpx;}
.question-title{line-height: 50rpx;font-size: 14px;font-weight: bold;}
.question-answer{width: 100%;}
.question-answer-list{min-height: 70rpx;line-height:40rpx;border: 1px solid #e8e8e8;margin: 20rpx 10rpx;padding: 10rpx 20rpx;border-radius: 10rpx;display: -webkit-box;display: -webkit-flex;display: flex;}
.answer-list-check{-webkit-box-flex: 1;-webkit-flex: 1;flex: 1;}
.answer-check-true{height:34rpx;width: 34rpx;margin:8rpx auto;background: #e93a30;text-align: center;line-height: 34rpx;color: #fff;border-radius:17rpx;}
.answer-check-false{height:34rpx;width: 34rpx;margin:8rpx auto;border: 1px solid #e8e8e8;border-radius:17rpx;}
.answer-list-text{-webkit-box-flex: 10;-webkit-flex: 10;flex: 10;padding-left: 10rpx;padding-top: 6rpx;}

