
.section-list{width: 96%;margin:10rpx auto;border-radius: 10rpx;background: #fff;}
.section-title{height: 30rpx;padding: 20rpx;}
.line-img{height: 30rpx;width: 10rpx;float: left;}
.line-img image{height: 100%;width: 100%;}
.section-title-text{height: 30rpx;float: left;margin-left: 20rpx;font-size: 14px;font-weight: bold;line-height: 30rpx;color: rgba(252, 152, 118, 1);}
.section-content{width:96%;display: -webkit-box;display: -webkit-flex;display: flex;margin:20rpx auto;}
.list-title{-webkit-box-flex: 2;-webkit-flex: 2;flex: 2;}
.list-title view{font-size: 14px;color: #000;line-height:60rpx;}
.list-content{-webkit-box-flex: 6;-webkit-flex: 6;flex: 6;}
.list-content view{font-size: 14px;color: #AAA;line-height:60rpx;}

