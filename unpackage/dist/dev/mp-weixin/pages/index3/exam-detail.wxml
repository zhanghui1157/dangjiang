<view class="content"><view class="main-content"><view class="test-top"><view class="test-info"><view>{{"考试名称："+info.exam_title}}</view><view>{{"考试人员："+info.communist_name}}</view><view>{{"考试时间："+info.exam_date}}</view><view>{{"考试时长："+info.exam_time+"分钟"}}</view></view><view class="test-score"><view class="test-score-text">得分/总分</view><view class="test-score-num">{{branch.log_score+"/"+info.exam_point}}</view></view></view><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index"><view class="question-list"><view class="question-title">{{index+1+"."+item.questions_title+"（"+item.questions_type+"）"}}</view><view class="question-answer"><block wx:for="{{item.questions_item}}" wx:for-item="val" wx:for-index="key"><label class="radio"><view class="question-answer-list"><view class="answer-list-text">{{val}}</view></view></label></block><view class="answer-decs">{{"我选择的答案是：【"+item.my_questions_item+"】；正确答案是【"+item.questions_answer+"】"}}</view></view></view></block></view></view>