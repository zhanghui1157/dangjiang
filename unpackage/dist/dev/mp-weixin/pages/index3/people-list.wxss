
.search-content{height: 100rpx;width: 100%;background: #f3f3f3;padding: 20rpx;}
.search-text{width: 96%;height: 60rpx;border-radius: 40rpx;background: #fff;margin: auto;padding:0 20rpx;font-size: 14px;}
.search-list{width: 100%;height: 100rpx;display: -webkit-box;display: -webkit-flex;display: flex;border-bottom: 1px solid #e8e8e8;}
.search-list-item{-webkit-box-flex: 1;-webkit-flex: 1;flex: 1;text-align: center;line-height: 100rpx;}
.dang-list{width: 100%;display: -webkit-box;display: -webkit-flex;display: flex;border-bottom: 1px solid #f0eded;padding: 10rpx 0;}
.dang-img{-webkit-box-flex: 1.5;-webkit-flex: 1.5;flex: 1.5;height: 80rpx;}
.dang-img image{height: 100%;width: 80rpx;margin: auto;border-radius: 50%;}
.dang-info{-webkit-box-flex: 8;-webkit-flex: 8;flex: 8;}
.dang-name{font-weight: bold;line-height: 40rpx;}
.dang-partno{color: #B3B3B3;line-height: 40rpx;}
.dang-check{-webkit-box-flex: 1;-webkit-flex: 1;flex: 1;}
.check-border{height: 50rpx;width: 50rpx;border-radius: 100%;border: 1px solid #e8e8e8;margin-top: 20rpx;}
.check-true{height: 30rpx;width: 30rpx;border-radius: 100%;background: #e93a30;margin: 8rpx 0 0 8rpx;}
.check-false{height: 30rpx;width: 30rpx;border-radius: 100%;background: #f3f3f3;margin: 8rpx 0 0 8rpx;}

