re" bindtap="__e"><view class="news-more-text icon iconfont">更多 </view></view></view></block><block wx:if="{{article_head.material_id}}"><view data-event-opts="{{[['tap',[['articleDetail',['$0'],['article_head.material_id']]]]]}}" class="video-head" bindtap="__e"><view class="video-head-img"><image src="{{article_head.material_thumb}}" mode></image></view><view class="video-head-title">{{article_head.material_title}}</view><view class="video-head-time">{{article_head.add_time}}</view></view></block><view class="video-content"><block wx:for="{{video_list}}" wx:for-item="item" wx:for-index="__i1__"><block wx:if="{{article_list}}"><view data-event-opts="{{[['tap',[['articleDetail',['$0'],[[['video_list','',__i1__,'material_id']]]]]]]}}" class="video-list" bindtap="__e"><view class="video-list-content"><image src="{{item.material_thumb}}" mode></image></view><view class="head-video-text list-text"><view class="head-video-title video-list-title">{{item.material_title}}</view><view class="head-video-time" style="text-align:left;">{{item.add_time}}</view></view></view></block></block></view><block wx:if="{{notes_list!=''}}"><view class="section-title"><view class="line-img"><image src="{{img[0]}}"></image></view><view class="section-title-text">学习实况</view><view data-event-opts="{{[['tap',[['notesList',['$0'],['topic_id']]]]]}}" class="news-more" bindtap="__e"><view class="news-more-text icon iconfont">更多 </view></view></view></block><block wx:if="{{notes_list!=''}}"><view class="notes-list"><block wx:for="{{notes_list}}" wx:for-item="item" wx:for-index="__i2__"><view data-event-opts="{{[['tap',[['notesDetail',['$0'],[[['notes_list','',__i2__,'notes_id']]]]]]]}}" class="notes-list-item" bindtap="__e"><view class="notes-title">{{item.notes_title}}</view><view class="notes-time">{{item.add_time}}</view></view></block></view></block></view><view class="footer"></view></view>